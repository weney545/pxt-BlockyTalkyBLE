{
    "name": "BlockyTalky-BLE",
    "version": "0.0.21",
    "description": "A key-value pair Bluetooth service for BlockyTalky",
    "license": "MIT",
    "dependencies": {
        "core": "*",
        "bluetooth": "*"
    },
    "files": [
        "README.md",
        "KeyValueService.cpp",
        "KeyValueService.h",
        "extension.cpp",
        "extension.ts",
        "shims.d.ts",
        "enums.d.ts"
    ],
    "testFiles": [
        "test.ts"
    ],
    "public": true,
    "targetVersions": {
        "target": "8.0.18",
        "targetId": "microbit"
    },
    "supportedTargets": [
        "microbit"
    ],
    "preferredEditor": "tsprj",
    "yotta": {
        "optionalConfig": {
            "microbit-dal": {
                "bluetooth": {
                    "open": 1,
                    "whitelist": 0
                }
            }
        }
    }
}
